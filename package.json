{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://alexisibarra.github.io/TheDarkSider_Frontend/",
  "dependencies": {
    "axios": "^0.18.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "gh-pages": "^2.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sass-convert": "^0.5.2",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "prestart": "yarn tailwind:css",
    "start": "npm-run-all -p watch-css start-js",
    "prestart-js": "yarn tailwind:css",
    "start-js": "react-scripts start",
    "prebuild": "yarn tailwind:css",
    "build": "npm-run-all build-css build-js",
    "prebuild-js": "yarn tailwind:css",
    "build-js": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --source-map true src/styles/styles.sass src/styles/styles.css --output-style compressed",
    "watch-css": "node-sass-chokidar --source-map true src/styles/styles.sass src/styles/styles.css --output-style compressed --watch",
    "tailwind:css": "tailwind build src/styles/tailwind.src.css -c tailwind.js -o src/styles/tailwind.css",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-app": "^4.0.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "tailwindcss": "^0.7.4"
  }
}